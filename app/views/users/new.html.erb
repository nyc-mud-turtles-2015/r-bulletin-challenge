<%= link_to "Login with Twitter!", "/auth/twitter" %>

<%= form_for @user, html: { multipart: true } do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.file_field :avatar %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit %>
<% end %>
